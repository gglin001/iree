    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch",
            "cwd": "${workspaceFolder}",
            "terminal": "console",
            "program": "${workspaceFolder}/build/install/bin/iree-run-module",
            "args": [
                "--device=local-task",
                // simple_abs_vmvx
                // "--module=demos/simple_abs_vmvx.vmfb",
                // "--function=abs",
                // "--input=f32=-5",
                // mnist
                "--module=demos/mnist/mnist.vmfb",
                "--function=predict",
                "--input=@demos/mnist_input.npy",
                "--output=-"
            ],
            "initCommands": [
                "settings set target.disable-aslr false",
                // "command source ${workspaceRoot}/.lldbinit",
                // "settings set target.process.thread.step-avoid-regexp (.)std(.)*",
            ],
            "env": {
                // "GLOG_v": "10",
            },
        },
    ]
}
